<% include ./partials/header.ejs %>

<h1>Homepage for restful blog app!</h1>

<% blogs.forEach(function(blog){ %>
  <div>
    <h2><%= blog.title %></h2>
    <img src="<%= blog.image %>" alt="">
    <span><%= blog.created %></span>
    <p><%= blog.body %></p>
  </div>
<% }) %>

<% include ./partials/footer.ejs %>